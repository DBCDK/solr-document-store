<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        version="3.2"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd">

  <enterprise-beans>
    <session>
      <ejb-name>SolrIndexerJS</ejb-name>
      <env-entry>
        <env-entry-name>moduleSearchPath</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>classpath:javascript/ classpath:javascript/javacore/ classpath:javascript/jscommon/system/ classpath:javascript/jscommon/convert/ classpath:javascript/jscommon/devel/ classpath:javascript/jscommon/util/ classpath:javascript/jscommon/external/ classpath:javascript/jscommon/marc/</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>scriptFile</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>javascript/SolrIndex.js</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>scriptFunction</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>createIndexData</env-entry-value>
      </env-entry>
    </session>
  </enterprise-beans>

</ejb-jar>
