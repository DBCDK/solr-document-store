[solr-doc-store-updater]

solrUrl=${SOLR_URL}
solrDocStoreUrl=${SOLR_DOC_STORE_URL}
queues=${QUEUES}
databaseConnectThrottle=${DATABASE_CONNECT_THROTTLE|1/s,5/m}
failureThrottle=${FAILURE_THROTTLE|2/100ms,5/500ms,10/s,20/m}
emptyQueueSleep=${EMPTY_QUEUE_SLEEP|10000}
rescanEvery=${RESCAN_EVERY|100}
idleRescanEvery=${IDLE_RESCAN_EVERY|5}
maxQueryTime=${MAX_QUERY_TIME|100}
threads=${THREADS|1}
